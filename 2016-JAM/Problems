***** Problema # 1

from time import time;
start = time()

res = 0
for i in range(1,1000):
	if i%3 == 0 or i%5 == 0:
		res = res + i 

end = time()
t = end-start

print (res)
print (t)

233168
0.0004019737243652344



***** Problema # 2

import numpy;
from time import time;
start = time()

oldFib = 1
Fib = 2
newFib = 0
sum = 2

while newFib < 4000000 :
	newFib = oldFib + Fib
	oldFib = Fib
	Fib = newFib
	if Fib%2 == 0:
		sum = sum + Fib

end = time()
t = end-start

print (sum)
print (t)

%%%
import numpy;
from time import time;
start = time()

oldFib = 1
Fib = 2
newFib = 0
sum = 4

while newFib < 34 :
	newFib = oldFib + Fib
	oldFib = Fib
	Fib = newFib
	sum = sum + Fib
	print (sum)
    
end = time()
t = end-start

print (sum/2)
print (t)
%%%

